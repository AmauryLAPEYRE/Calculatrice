{
  "name": "fydo-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@anthropic-ai/sdk": "^0.50.3",
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.3.0",
    "@supabase/supabase-js": "^2.49.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "detect-port-alt": "^1.1.6",
    "express": "^5.1.0",
    "firebase": "^11.6.0",
    "lucide-react": "^0.503.0",
    "multer": "^1.4.5-lts.2",
    "quagga": "^0.12.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.5.1",
    "react-scripts": "5.0.1",
    "stripe": "^18.1.0",
    "supabase": "^2.22.12",
    "uuid": "^11.1.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "set GENERATE_SOURCEMAP=false && react-scripts start",
    "start:clean": "set BROWSER=none && set GENERATE_SOURCEMAP=false && react-scripts start",
    "start:cors": "set BROWSER=none && set GENERATE_SOURCEMAP=false && set REACT_APP_USE_FIREBASE_EMULATORS=false && react-scripts start",
    "dev": "npm run start:cors",
    "build": "set GENERATE_SOURCEMAP=false && react-scripts build",
    "build:prod": "set GENERATE_SOURCEMAP=false && set REACT_APP_USE_FIREBASE_EMULATORS=false && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serve": "npm run build && npx serve -s build -l 3000",
    "clear-cache": "rmdir /s node_modules && del package-lock.json && npm install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7"
  }
}
